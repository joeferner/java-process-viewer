{
  "name": "java-process-viewer-public",
  "version": "0.0.1",
  "description": "Java Process Viewer - Public",
  "private": true,
  "keywords": [
    "java"
  ],
  "author": "Joe Ferner <joe@fernsroth.com>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=prod webpack --color --progress",
    "build:watch": "cross-env NODE_ENV=dev webpack --watch --color --progress",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "eslint --fix src/**/*.ts",
    "test": "jest --config jestconfig.json",
    "test:watch": "jest --watchAll --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run precommit",
    "precommit": "npm run format && npm run lint"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@types/react-beautiful-dnd": "^11.0.4",
    "@welldone-software/why-did-you-render": "^4.0.0",
    "axios": "^0.19.1",
    "clsx": "^1.0.4",
    "debounce": "^1.2.0",
    "jps-parser": "0.0.x",
    "jstack-parser": "0.0.x",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^12.2.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/debounce": "^1.2.0",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "prettier": "^1.19.1",
    "terser-webpack-plugin": "^2.3.2",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "worker-loader": "^2.0.0"
  }
}
